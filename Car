class Bus{
    String str1 = "1번 버스객체 만들어짐!";
    String str2 = "2번 버스객체 만들어짐!";
    String str3 = "탑승 승객 수 = 2";
    String str4 = "잔여 승객 수 = 28";
    String str5 = "요금 확인 = 1500";
    String str6 = "주유량 : 50";
    String str7 = "상태 : 차고지행";
    String str8 = "주유량 : 60";
    String str9 = "최대 승객 수를 초과했습니다.";
    String str10 = "탑승 승객 수 = 5";
    String str11 = "잔여 승객 수 = 23";
    String str12 = "요금 확인 = 1500";
    String str13 = "주유량 : 5";
    String str14 = "주유가 필요합니다.";
    String str15 = "상태 : 운행";
}

public class Car{
    private Taxi t;

    private Bus b;

    public Taxi getT() {
        return t;
    }

    public void setT(Taxi t) {
        this.t = t;
    }

    public Bus getB() {
        return b;
    }

    public void setB(Bus b) {
        this.b = b;
    }

    public static void main(String[] args) {
        Car c = new Car();
        System.out.println(c.Taxi());
        System.out.println(c.Bus());
    }
}
